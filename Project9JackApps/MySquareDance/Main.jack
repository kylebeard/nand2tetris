class Main {
    function void main(){
        var int i;
        let i = 1;
        while(i < 400){
            do Screen.setColor(false);
            do Screen.drawRectangle(i-1,0,100+(i-1), 100);
            do Screen.setColor(true);
            do Screen.drawRectangle(i,0,100+i,100);
            let i = i + 1;
            do Sys.wait(10);
            do Screen.clearScreen();
        }
        return;
    }
}